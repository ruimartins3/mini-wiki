{
    "name": "mini-wiki",          
    "builds": [
      {
        "src": "public/*",        
        "use": "@vercel/static"   
      }
    ],
    "routes": [
      {
        "handle": "filesystem"     
      },
      {
        "src": "/(.*)",            
        "dest": "public/index.html" 
      }
    ]
  }
  